{
  "name": "@schoolos/integrations",
  "version": "0.1.0",
  "private": true,
  "description": "SchoolOS Phase 4 integrations package - SIS and calendar connectors with sync orchestration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --clean --tsconfig tsconfig.json",
    "dev": "tsup src/index.ts --format cjs,esm --watch --tsconfig tsconfig.json",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.7.7",
    "bottleneck": "^2.19.5",
    "bullmq": "^5.7.15",
    "date-fns": "^3.6.0",
    "googleapis": "^144.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "nock": "^13.5.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "@schoolos/database": "*"
  }
}
